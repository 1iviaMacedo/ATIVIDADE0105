2.
DELIMITER //

CREATE PROCEDURE SP_ConsultarLocacoesPorCarro
    (IN CodigoCarro INT)
BEGIN
    SELECT *
    FROM locações
    WHERE CODCARRO = CodigoCarro;
END //

DELIMITER ;





