5. Crie uma stored Procedure para o exercício 2,3,4 e 6

2.
DELIMITER //

CREATE PROCEDURE SP_ConsultarLocacoesPorCarro
    (IN CodigoCarro INT)
BEGIN
    SELECT *
    FROM locações
    WHERE CODCARRO = SELECT * FROM `LOCAÇÕES` WHERE CODCARRO = 3';
END //

DELIMITER ;





3.
DELIMITER //

CREATE PROCEDURE SP_ConsultarLocacoesPorCarro
    (IN CodigoCarro INT)
BEGIN
    SELECT *
    FROM locações
    WHERE CODCARRO = SELECT * FROM `CARROS`, `CLIENTES`, `LOCAÇÕES` WHERE COMBUSTÍVEL = 'GASOLINA' OR COMBUSTÍVEL = 'ETANOL';
END //

DELIMITER ;





4.
DELIMITER //

CREATE PROCEDURE SP_ConsultarLocacoesPorCarro
    (IN CodigoCarro INT)
BEGIN
    SELECT *
    FROM locações
    WHERE CODCARRO = SELECT * FROM `CARROS` WHERE ESTÁ_DISPONIVEL='SIM';
END //

DELIMITER ;





6.
DELIMITER //

CREATE PROCEDURE SP_ConsultarLocacoesPorCarro
    (IN CodigoCarro INT)
BEGIN
    SELECT *
    FROM locações
    WHERE CODCARRO = INSERT INTO `CLIENTES`(`CÓDIGO`, `NOME_CLIENTE`, `TELEFONE`, `SEXO`, `IDADE`) VALUES ('25','AMANDA','(19)9955-7838','F','37');
END //

DELIMITER ;

